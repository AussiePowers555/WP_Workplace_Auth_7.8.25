    "build:trace": "next build --experimental-turbo-trace",
    "build:analyze": "cross-env ANALYZE_BUNDLE=true npm run build",
    "start": "next start -p $PORT",
    "start:prod": "cross-env NODE_ENV=production PORT=${PORT:-3000} next start -p 3000",
    "start:windows": "next start -p 3000",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "lint:fix": "next lint --fix",
    "test:full": "npm run lint && npm run typecheck",
    "deploy": "./deploy.sh",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop whitepointer-app",
    "pm2:restart": "pm2 restart whitepointer-app",
    "pm2:logs": "pm2 logs whitepointer-app",
    "backup-db": "cp pbike-rescue.db backup-$(date +%Y%m%d-%H%M%S).db"
