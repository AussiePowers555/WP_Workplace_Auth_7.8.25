{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 9015",
    "dev:webpack": "next dev -p 9015",
    "dev:clear": "next dev --turbopack -p 9015 --clear",
    "setup-local-ip": "node setup-local-ip.js",
    "dev:auto-restart": "node auto-restart-dev.js",
    "tunnel": "node start-cloudflare-tunnel.js",
